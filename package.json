{
  "name": "ironman-react",
  "version": "1.0.0",
  "description": "Ironman training log",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm-run-all update-schema --parallel client server --parallel postclient postserver",
    "client": "webpack-dev-server --inline --content-base . --history-api-fallback",
    "server": "nodemon server/index.js --watch server",
    "lint": "eslint client server util devServer.js",
    "update-schema": "babel-node ./util/update-schema.js",
    "prebuild": "rimraf build/",
    "build": "npm-run-all --parallel build:client build:server",
    "build:client": "cross-env NODE_ENV='\"production\"' webpack --config webpack.config.prod.js --progress",
    "build:server": "babel server -d build/server",
    "deploy": "rimraf backup && mv deploy backup && mv build deploy && pm2 restart ironman",
    "deploy:undo": "rimraf deploy && cp backup deploy -rf && pm2 restart ironman",
    "publish": "npm run build && npm run deploy"
  },
  "author": "david lemvigh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dlemvigh/IronmanReact/"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "express-graphql": "^0.5.4",
    "file-loader": "^0.9.0",
    "graphql": "^0.7.2",
    "graphql-custom-datetype": "^0.3.0",
    "graphql-relay": "^0.4.3",
    "jquery": "^3.1.1",
    "left-pad": "^1.1.3",
    "lodash": "^4.16.4",
    "mobile-detect": "^1.3.5",
    "moment": "^2.15.1",
    "mongoose": "^4.6.5",
    "node-sass": "^3.13.0",
    "npm-run-all": "^3.1.1",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.7",
    "react-css-transition-replace": "^2.1.0",
    "react-datetime": "^2.6.0",
    "react-dom": "^15.3.2",
    "react-facebook-login": "^3.3.2",
    "react-relay": "^0.9.3",
    "react-router": "^2.8.1",
    "react-router-bootstrap": "^0.23.1",
    "react-router-relay": "^0.13.5",
    "title-case": "^2.1.0",
    "toastr": "^2.1.2",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-relay-plugin": "^0.9.3",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "eslint": "^3.11.1",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "favicons-webpack-plugin": "0.0.7",
    "html-webpack-plugin": "^2.24.1",
    "http-server": "^0.9.0",
    "nodemon": "^1.11.0",
    "react-css-modules": "^3.7.10",
    "react-hot-loader": "^3.0.0-beta.6",
    "resolve-url": "^0.2.1",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
