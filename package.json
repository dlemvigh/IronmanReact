{
  "name": "ironman-react",
  "version": "1.0.0",
  "description": "Ironman training log",
  "main": "index.js",
  "scripts": {
    "run:test": "cross-env NODE_ENV='\"test\"' node build/server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack",
    "start:old": "cross-env NODE_ENV='\"dev\"' npm-run-all update-schema --parallel client server --parallel postclient postserver",
    "start:test": "cross-env NODE_ENV='\"test\"' npm-run-all update-schema --parallel client server --parallel postclient postserver",
    "client": "webpack-dev-server --inline --content-base . --history-api-fallback",
    "server": "nodemon server/index.js --watch server",
    "server:prod": "nodemon build/server/index.js --watch build",
    "lint": "eslint client server util devServer.js",
    "update-schema": "babel-node ./util/update-schema.js",
    "prebuild": "rimraf build/",
    "build": "npm-run-all --parallel build:client build:server build:shared",
    "build:test": "npm-run-all --parallel build:client:test build:server build:shared",
    "build:client": "cross-env NODE_ENV='\"production\"' webpack --config webpack.config.prod.js --progress",
    "build:client:test": "cross-env NODE_ENV='\"test\"' webpack --config webpack.config.test.js --progress",
    "build:server": "babel server -d build/server",
    "build:shared": "babel shared -d build/shared",
    "deploy": "rimraf backup && mv deploy backup && mv build deploy && pm2 restart ironman",
    "deploy:undo": "rimraf deploy && cp backup deploy -rf && pm2 restart ironman",
    "publish": "npm run build && npm run deploy"
  },
  "author": "david lemvigh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dlemvigh/IronmanReact/"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/register": "^7.4.4",
    "bootstrap": "^3.3.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.0",
    "express-graphql": "^0.8.0",
    "graphql": "^14.3.0",
    "graphql-custom-datetype": "^0.5.0",
    "graphql-relay": "^0.6.0",
    "jquery": "^3.4.1",
    "lodash": "^4.17.11",
    "mobile-detect": "^1.4.3",
    "moment": "^2.24.0",
    "mongoose": "^5.5.9",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-bootstrap": "^0.30.7",
    "react-css-transition-replace": "^3.0.0",
    "react-datetime": "^2.12.0",
    "react-dom": "^16.8.6",
    "react-ga": "^2.5.7",
    "react-loading-bar": "0.0.7",
    "react-router": "^3.2.0",
    "react-router-bootstrap": "^0.23.1",
    "recharts": "^0.20.5",
    "title-case": "^2.1.0",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "lodash-webpack-plugin": "^0.11.5",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.0",
    "react-css-modules": "^4.7.11",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.31.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpackbar": "^3.2.0"
  }
}
